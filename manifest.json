{
  "manifest_version": 3,
  "name": "HueCube - YouTube Tab Opener",
  "version": "1.0",
  "description": "Opens a YouTube video in a (special) New Tab",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}